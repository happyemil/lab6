/*
	P=U^2/R
*/
float calc_power_r(float volt, float resistance){
	float P = 0.0;
	P = (volt * 2)/resistance;
	return P;
}

/*
	P=U*I
*/
float calc_power_i(float volt, float current){
	float P = 0.0;
	P = volt * resistance;
	return P;
}
